<?xml version="1.0" encoding="UTF-8"?>
<model:OzobotProgram
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:model="http://www.example.org/model"
    xsi:schemaLocation="http://www.example.org/model ../ozobot.model/model/model.ecore">
  <commandSet
      xsi:type="model:Move"
      distance="20"/>
  <commandSet
      xsi:type="model:Light"
      color="green"/>
  <commandSet
      xsi:type="model:Wait"
      time="20"/>
  <commandSet
      xsi:type="model:Repeat"
      count="2">
    <repeatCommands
        xsi:type="model:Move"
        distance="15"/>
    <repeatCommands
        xsi:type="model:Rotate"
        angle="90.0"
        speed="medium"/>
    <repeatCommands
        xsi:type="model:Wait"
        time="25"/>
  </commandSet>
  <commandSet
      xsi:type="model:Move"
      distance="20"
      velocity="fast"/>
  <commandSet
      xsi:type="model:Rotate"
      angle="45.0"
      direction="Right"
      speed="fast"/>
</model:OzobotProgram>
