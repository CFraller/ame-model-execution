<?xml version="1.0" encoding="UTF-8"?>
<model:Ozobot xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:model="http://ozobot.xdsml.ozobotl/model/" xsi:schemaLocation="http://www.example.org/model" name="Ozobot">
  <programs name="OzobotProgram" current="//@programs.0/@block/@commands.0">
    <block name="Block1">
      <commands xsi:type="model:Move" name="Move1" outgoing="//@programs.0/@block/@transitions.0" distance="5"/>
      <commands xsi:type="model:Light" name="Light1" outgoing="//@programs.0/@block/@transitions.1" incoming="//@programs.0/@block/@transitions.0" color="red"/>
      <commands xsi:type="model:Move" name="Move2" outgoing="//@programs.0/@block/@transitions.2" incoming="//@programs.0/@block/@transitions.1" distance="5" velocity="medium"/>
      <commands xsi:type="model:Repeat" name="Repeat1" outgoing="//@programs.0/@block/@transitions.6" incoming="//@programs.0/@block/@transitions.2" count="3">
        <block name="Block2">
          <commands xsi:type="model:Rotate" name="Rotate1" outgoing="//@programs.0/@block/@commands.3/@block/@transitions.0" angle="45.0"/>
          <commands xsi:type="model:Light" name="Light2" incoming="//@programs.0/@block/@commands.3/@block/@transitions.0" color="blue"/>
          <transitions source="//@programs.0/@block/@commands.3/@block/@commands.0" target="//@programs.0/@block/@commands.3/@block/@commands.1"/>
        </block>
      </commands>
      <commands xsi:type="model:Wait" name="Wait1" outgoing="//@programs.0/@block/@transitions.3" incoming="//@programs.0/@block/@transitions.6" time="20"/>
      <commands xsi:type="model:Move" name="Move3" outgoing="//@programs.0/@block/@transitions.4" incoming="//@programs.0/@block/@transitions.3" distance="10" velocity="fast"/>
      <commands xsi:type="model:Rotate" name="Rotate2" outgoing="//@programs.0/@block/@transitions.5" incoming="//@programs.0/@block/@transitions.4" velocity="slow" angle="45.0"/>
      <commands xsi:type="model:Light" name="Light3" incoming="//@programs.0/@block/@transitions.5" color="yellow"/>
      <transitions source="//@programs.0/@block/@commands.0" target="//@programs.0/@block/@commands.1"/>
      <transitions source="//@programs.0/@block/@commands.1" target="//@programs.0/@block/@commands.2"/>
      <transitions source="//@programs.0/@block/@commands.2" target="//@programs.0/@block/@commands.3"/>
      <transitions source="//@programs.0/@block/@commands.4" target="//@programs.0/@block/@commands.5"/>
      <transitions source="//@programs.0/@block/@commands.5" target="//@programs.0/@block/@commands.6"/>
      <transitions source="//@programs.0/@block/@commands.6" target="//@programs.0/@block/@commands.7"/>
      <transitions source="//@programs.0/@block/@commands.3" target="//@programs.0/@block/@commands.4"/>
    </block>
  </programs>
</model:Ozobot>
